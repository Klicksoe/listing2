Require valid-user
AuthType Basic
AuthName "Restricted Area"
AuthUserFile ..\htpasswd
#AuthGroupFile /dev/null

<IfModule mod_rewrite.c>
    Options -MultiViews

    RewriteEngine On
    RewriteBase /silextest/web
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^ index.php [QSA,L]
</IfModule>